// 建立认识关系(非朋友关系) - 无数字格式问题
MATCH (a:人物 {姓名: '张三'}), (b:人物 {姓名: '周九'}) CREATE (a)-[:认识 {认识时间: '2020-03-01', 场合: '行业会议'}]->(b);
MATCH (a:人物 {姓名: '李四'}), (b:人物 {姓名: '吴十'}) CREATE (a)-[:认识 {认识时间: '2019-09-01', 场合: '商务合作'}]->(b);
MATCH (a:人物 {姓名: '王五'}), (b:人物 {姓名: '孙八'}) CREATE (a)-[:认识 {认识时间: '2020-01-01', 场合: '技术沙龙'}]->(b);