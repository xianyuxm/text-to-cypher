// 人物与公司关系 - 无数字格式问题
MATCH (a:人物 {姓名: '张三'}), (b:公司 {名称: '阿里巴巴'}) CREATE (a)-[:工作于 {职位: '高级软件工程师', 入职时间: '2018-07-01', 部门: '淘宝事业部'}]->(b);
MATCH (a:人物 {姓名: '李四'}), (b:公司 {名称: '腾讯'}) CREATE (a)-[:工作于 {职位: '产品总监', 入职时间: '2017-03-01', 部门: '微信事业部'}]->(b);
MATCH (a:人物 {姓名: '赵六'}), (b:公司 {名称: '字节跳动'}) CREATE (a)-[:工作于 {职位: '技术架构师', 入职时间: '2019-05-01', 部门: '抖音事业部'}]->(b);
MATCH (a:人物 {姓名: '孙八'}), (b:公司 {名称: '百度'}) CREATE (a)-[:工作于 {职位: 'CTO', 入职时间: '2016-01-01', 部门: 'AI实验室'}]->(b);
MATCH (a:人物 {姓名: '周九'}), (b:公司 {名称: '阿里巴巴'}) CREATE (a)-[:工作于 {职位: '市场总监', 入职时间: '2018-11-01', 部门: '市场部'}]->(b);

// 人物与学校关系 - 无数字格式问题
MATCH (a:人物 {姓名: '张三'}), (b:学校 {名称: '北京大学'}) CREATE (a)-[:毕业于 {专业: '计算机科学', 毕业年份: 2016, 学历: '硕士'}]->(b);
MATCH (a:人物 {姓名: '李四'}), (b:学校 {名称: '清华大学'}) CREATE (a)-[:毕业于 {专业: '工商管理', 毕业年份: 2014, 学历: '硕士'}]->(b);
MATCH (a:人物 {姓名: '王五'}), (b:学校 {名称: '复旦大学'}) CREATE (a)-[:毕业于 {专业: '统计学', 毕业年份: 2018, 学历: '本科'}]->(b);
MATCH (a:人物 {姓名: '赵六'}), (b:学校 {名称: '清华大学'}) CREATE (a)-[:毕业于 {专业: '软件工程', 毕业年份: 2015, 学历: '博士'}]->(b);
MATCH (a:人物 {姓名: '钱七'}), (b:学校 {名称: '北京大学'}) CREATE (a)-[:毕业于 {专业: '艺术设计', 毕业年份: 2017, 学历: '本科'}]->(b);
