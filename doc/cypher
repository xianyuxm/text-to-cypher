// 建立关系 - 无数字格式问题
MATCH (a:人物 {姓名: '张三'}), (b:人物 {姓名: '李四'}) CREATE (a)-[:朋友 {认识时间: '2018-05-01', 亲密程度: 8}]->(b);
MATCH (a:人物 {姓名: '张三'}), (b:人物 {姓名: '王五'}) CREATE (a)-[:朋友 {认识时间: '2019-03-15', 亲密程度: 7}]->(b);
MATCH (a:人物 {姓名: '李四'}), (b:人物 {姓名: '赵六'}) CREATE (a)-[:朋友 {认识时间: '2017-08-20', 亲密程度: 9}]->(b);
MATCH (a:人物 {姓名: '王五'}), (b:人物 {姓名: '钱七'}) CREATE (a)-[:朋友 {认识时间: '2020-01-10', 亲密程度: 6}]->(b);
MATCH (a:人物 {姓名: '赵六'}), (b:人物 {姓名: '孙八'}) CREATE (a)-[:朋友 {认识时间: '2016-11-05', 亲密程度: 8}]->(b);
MATCH (a:人物 {姓名: '钱七'}), (b:人物 {姓名: '周九'}) CREATE (a)-[:朋友 {认识时间: '2019-07-22', 亲密程度: 7}]->(b);
MATCH (a:人物 {姓名: '孙八'}), (b:人物 {姓名: '吴十'}) CREATE (a)-[:朋友 {认识时间: '2015-09-30', 亲密程度: 9}]->(b);

// 人物与地点关系 - 无数字格式问题
MATCH (a:人物 {姓名: '张三'}), (b:地点 {名称: '北京'}) CREATE (a)-[:住在 {开始时间: '2018-01-01', 住址: '朝阳区建国路88号'}]->(b);
